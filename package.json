{
  "name": "acme-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "build": "next build",
    "build:az": "next build && cp -r .next/static .next/standalone/static/ && cp -r public .next/standalone/",
    "clean": "git clean -xdf node_modules .next",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/server/db/seed.ts",
    "db:seedd": "tsx src/server/db/index.ts",
    "ui-add": "pnpm dlx shadcn add && prettier src --write --list-different",
    "dev": "next dev --turbopack",
    "format": "prettier --check . !pnpm-lock.yaml !package.json",
    "format:fix": "prettier --write .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "start": "node .next/standalone/server.js"
  },
  "dependencies": {
    "@clerk/elements": "^0.22.0",
    "@clerk/nextjs": "^6.9.0",
    "@hookform/resolvers": "^3.9.1",
    "@neondatabase/serverless": "^0.10.4",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@t3-oss/env-nextjs": "^0.11.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.0",
    "drizzle-seed": "^0.1.2",
    "drizzle-zod": "^0.6.0",
    "lucide-react": "^0.468.0",
    "next": "15.1.0",
    "next-international": "^1.3.1",
    "next-safe-action": "^7.9.9",
    "next-themes": "^0.4.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.0",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@types/eslint": "^8.56.12",
    "@types/node": "^22.10.1",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "eslint": "^8.57.1",
    "drizzle-kit": "^0.30.0",
    "eslint-config-next": "15.1.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
