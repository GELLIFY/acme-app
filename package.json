{
  "name": "acme-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "build": "next build",
    "clean": "git clean -xdf node_modules .next",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/server/db/seed.ts",
    "db:seedd": "tsx src/server/db/index.ts",
    "ui-add": "pnpm dlx shadcn add && prettier src --write --list-different",
    "dev": "next dev --turbopack",
    "format": "prettier --check . !pnpm-lock.yaml !package.json",
    "format:fix": "prettier --write .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "start": "node .next/standalone/server.js"
  },
  "dependencies": {
    "@clerk/elements": "^0.23.1",
    "@clerk/nextjs": "^6.12.2",
    "@hookform/resolvers": "^3.10.0",
    "@neondatabase/serverless": "^0.10.4",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@t3-oss/env-nextjs": "^0.12.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.0",
    "drizzle-seed": "^0.3.1",
    "drizzle-zod": "^0.7.0",
    "lucide-react": "^0.477.0",
    "next": "15.2.0",
    "next-international": "^1.3.1",
    "next-safe-action": "^7.10.4",
    "next-themes": "^0.4.4",
    "pg": "^8.13.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.9",
    "tw-animate-css": "^1.2.4",
    "ws": "^8.18.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@tailwindcss/postcss": "^4.0.9",
    "@types/eslint": "^8.56.12",
    "@types/node": "^22.13.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "drizzle-kit": "^0.30.5",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.2.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3"
  }
}
